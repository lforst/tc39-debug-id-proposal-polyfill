{
  "name": "vite-plugin-proposal-debug-id",
  "version": "1.0.0",
  "description": "Polyfill Vite Plugin for the TC39 Source Map Debug ID Proposal",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lforst/tc39-debug-id-proposal-polyfill.git"
  },
  "bugs": {
    "url": "https://github.com/lforst/tc39-debug-id-proposal-polyfill/issues"
  },
  "homepage": "https://github.com/lforst/tc39-debug-id-proposal-polyfill#readme",
  "author": "Luca Forstner",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "run-p build:transpile build:types",
    "build:transpile": "rollup --config rollup.config.mjs",
    "build:types": "tsc --project types.tsconfig.json"
  },
  "dependencies": {
    "magic-string": "^0.30.5"
  },
  "peerDependencies": {
    "vite": ">=4"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@babel/preset-env": "7.23.2",
    "@babel/preset-typescript": "7.23.2",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@types/node": "^16",
    "rollup": "4.1.1",
    "vite": "^4.4.11",
    "typescript": "5.2.2",
    "proposal-debug-id-utils": "1.0.0"
  },
  "engines": {
    "node": ">= 16"
  }
}
